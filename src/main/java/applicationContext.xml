<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
    <bean id="rhinoConsole" class="org.geoserver.web.DemoLinkInfo">
        <property name="id" value="rhinoConsole"/>
        <property name="titleKey" value="RhinoConsolePage.title"/>
        <property name="descriptionKey" value="RhinoConsolePage.shortDescription"/>
        <property name="componentClass" value="org.geoserver.geoscript.javascript.web.RhinoConsolePage"/>
    </bean>
    <bean id="JSTransactionPlugin" class="org.geoserver.geoscript.javascript.wfs.JavaScriptTransactionPlugin">
        <constructor-arg ref="JSModules"/>
    </bean>
    <bean id="JSModules" class="org.geoserver.geoscript.javascript.JavaScriptModules">
        <constructor-arg ref="resourceLoader"/>
    </bean>
</beans>
